&soc {
	qcom,msm-thermal-simple {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "qcom,msm-thermal-simple";
		qcom,thermal-vadc = <&pm660_vadc>;
		qcom,adc-channel = <0x4c>;
		qcom,poll-ms = <3000>;
		qcom,start-delay = <60>;
		zone@0 {
			reg = <0>;
			qcom,silver-khz = <1843200>;
			qcom,gold-khz = <2150400>;
			qcom,trip-deg = <63>;
		};

		zone@1 {
			reg = <1>;
			qcom,silver-khz = <1747200>;
			qcom,gold-khz = <2150400>;
			qcom,trip-deg = <65>;
		};

		zone@2 {
			reg = <2>;
			qcom,silver-khz = <1747200>;
			qcom,gold-khz = <1958400>;
			qcom,trip-deg = <66>;
		};

		zone@3 {
			reg = <3>;
			qcom,silver-khz = <1747200>;
			qcom,gold-khz = <1401600>;
			qcom,trip-deg = <67>;
		};

		zone@4 {
			reg = <4>;
			qcom,silver-khz = <1401600>;
			qcom,gold-khz = <1401600>;
			qcom,trip-deg = <71>;
		};

		zone@5 {
			reg = <5>;
			qcom,silver-khz = <1401600>;
			qcom,gold-khz = <1113600>;
			qcom,trip-deg = <73>;
		};

		zone@6 {
			reg = <6>;
			qcom,silver-khz = <1113609>;
			qcom,gold-khz = <902400>;
			qcom,trip-deg = <75>;
		};
	};
};
